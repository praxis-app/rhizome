{
  "name": "rhizome",
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node --project ./tsconfig.src.json src/main.ts",
    "start:client": "vite --host",
    "build": "tsc -b && eslint src --ext ts && tsc && babel -x .ts -d dist src",
    "build:client": "tsc -b && eslint view --ext ts,tsx && vite build --emptyOutDir true",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typeorm": "tsx --tsconfig ./tsconfig.src.json ./node_modules/typeorm/cli.js",
    "typeorm:gen": "npm run typeorm migration:generate -- -p -d ./src/database/data-source.ts",
    "typeorm:run": "npm run typeorm migration:run -- -d ./src/database/data-source.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "axios": "^1.7.8",
    "body-parser": "^2.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.0.1",
    "react-use-websocket": "^4.10.0",
    "redis": "^4.7.0",
    "typeorm": "^0.3.14",
    "url": "^0.11.4",
    "uuid": "^10.0.0",
    "ws": "^8.18.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.25.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "babel-plugin-add-import-extension": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.11",
    "vite-plugin-dynamic-import": "^1.5.0"
  }
}
